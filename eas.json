{
  "cli": {
    "version": ">= 0.54.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "buildConfiguration": "Release",
        "resourceClass": "m-medium",
        "cocoapods": "1.11.3",
        "scheme": "HydroApp"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "${APPLE_ID}",
        "ascAppId": "${APP_STORE_CONNECT_APP_ID}",
        "appleTeamId": "${APPLE_TEAM_ID}"
      }
    }
  },
  "updates": {
    "url": "https://u.expo.dev/${EAS_PROJECT_ID}"
  }
}
